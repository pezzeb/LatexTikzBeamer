\mode<presentation>

\definecolor{FiEblue}{RGB}{0,185,231}
\definecolor{FiEturquoise}{RGB}{23,199,210}
\definecolor{FiEgreen}{RGB}{0,207,181}

\setbeamertemplate{sections/subsections in toc}[square]
\setbeamertemplate{items}[square]

\setbeamertemplate{itemize item}{\color{FiEblue!70}$\blacksquare$}
\setbeamertemplate{itemize subitem}{\color{FiEturquoise!70}$\blacktriangleright$}
\setbeamertemplate{itemize subsubitem}{\color{FiEgreen!70}$\bullet$}

\setbeamertemplate{enumerate items}[default]
\setbeamercolor{enumerate item}{fg=black}
\setbeamercolor{enumerate subitem}{fg=black}
\setbeamercolor{enumerate subsubitem}{fg=black}

\mode<all>
